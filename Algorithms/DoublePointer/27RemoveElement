class Solution {
    public int removeElement(int[] nums, int val) {
        if (nums.length == 0 || nums == null) return 0;
        
        int start = 0;
        for(int i = 0; i < nums.length; i++){
            if(nums[i] != val){
                nums[start] = nums[i];
                start++;
            }
        }
        return start;
    }
}

------------------------------------------
Time: O(N)
Space: O(1)
